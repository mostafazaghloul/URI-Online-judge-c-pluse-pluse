#include <iostream>
#include <iomanip>
#include <cmath>
  
using namespace std;
  
int main() {
  
    /**
     * Escreva a sua solução aqui
     * Code your solution here
     * Escriba su solución aquí
     */
    int R;
    const double pi=3.14159;
    double VOLUME;
    cin>>R;
    VOLUME=(4/3.0)*(pi)*pow(R,3);
 
    cout<<fixed<<setprecision(3);
    cout<<"VOLUME = "<<VOLUME<<"\n";
    return 0;
 
  }
